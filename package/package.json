{
  "name": "@apollo/experimental-nextjs-app-support",
  "version": "0.8.0",
  "repository": {
    "url": "git+https://github.com/apollographql/apollo-client-nextjs"
  },
  "keywords": [
    "apollo",
    "nextjs",
    "apollo-client",
    "graphql",
    "ssr",
    "rsc",
    "app-router",
    "app"
  ],
  "type": "module",
  "exports": {
    "./rsc": {
      "require": {
        "types": "./dist/rsc/index.d.cts",
        "react-server": "./dist/rsc/index.cjs",
        "default": "./dist/empty.cjs"
      },
      "import": {
        "types": "./dist/rsc/index.d.ts",
        "react-server": "./dist/rsc/index.js",
        "default": "./dist/empty.js"
      }
    },
    "./ssr": {
      "require": {
        "types": "./dist/ssr/next.ssr.d.cts",
        "react-server": "./dist/empty.cjs",
        "node": "./dist/ssr/next.ssr.cjs",
        "browser": "./dist/ssr/next.browser.cjs",
        "default": "./dist/empty.cjs"
      },
      "import": {
        "types": "./dist/ssr/next.ssr.d.ts",
        "react-server": "./dist/empty.js",
        "node": "./dist/ssr/next.ssr.js",
        "browser": "./dist/ssr/next.browser.js",
        "default": "./dist/empty.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "ssr": [
        "./dist/ssr/next.ssr.d.ts"
      ],
      "rsc": [
        "./dist/rsc/index.d.ts"
      ]
    }
  },
  "author": "packages@apollographql.com",
  "license": "MIT",
  "files": [
    "dist/",
    "package.json",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist; tsup",
    "test": "concurrently -c auto \"yarn:test:*(!base) $@\"",
    "test:base": "node --loader tsx --no-warnings --test \"$@\" src/**/*.test.ts",
    "test:ssr": "NODE_OPTIONS=\"$NODE_OPTIONS --conditions=node\" yarn run test:base",
    "test:browser": "NODE_OPTIONS=\"$NODE_OPTIONS --conditions=browser\" yarn run test:base",
    "test:rsc": "NODE_OPTIONS=\"$NODE_OPTIONS --conditions=react-server\" yarn run test:base",
    "prepack": "yarn test run && yarn build",
    "test-bundle": "attw --pack ."
  },
  "devDependencies": {
    "@apollo/client": "3.9.1",
    "@arethetypeswrong/cli": "0.13.6",
    "@total-typescript/shoehorn": "0.1.1",
    "@tsconfig/recommended": "1.0.3",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "concurrently": "^8.2.2",
    "eslint": "8.56.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "^10.3.10",
    "globstar": "^1.0.0",
    "graphql": "16.8.1",
    "next": "14.1.0",
    "react": "18.3.0-canary-60a927d04-20240113",
    "react-dom": "18.3.0-canary-60a927d04-20240113",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "tsup": "8.0.1",
    "tsx": "^4.7.0",
    "typescript": "5.3.3",
    "vitest": "1.2.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.9.0",
    "next": "^13.4.1 || ^14.0.0",
    "react": "^18"
  },
  "dependencies": {
    "rehackt": "^0.0.4",
    "server-only": "^0.0.1",
    "superjson": "^1.12.2 || ^2.0.0",
    "ts-invariant": "^0.10.3"
  }
}
